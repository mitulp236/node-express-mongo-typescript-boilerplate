{
    "swagger": "2.0",
    "info": {
        "title": "Node-mongo-typescript boilerplace",
        "description": "for changing project edit swagger.json file on root directory",
        "version": "1.0"
    },
    "host": "localhost:5000",
    "basePath": "/api",
    "produces": [
        "application/json"
    ],
    "paths": {
        "/login": {
            "post": {
                "x-swagger-router-controller": "home",
                "operationId": "index",
                "tags": [
                    "/auth"
                ],
                "description": "",
                "parameters": [
                    {
                        "name": "email",
                        "type": "string",
                        "required": true                        
                    },
                    {
                        "name": "password",
                        "type": "string",
                        "required": true                        
                    }
                ],
                "responses": {}
            }
        }
        
    }
}